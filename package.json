{
  "name": "skorporaal.astro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:astro": "astro build",
    "check:lint": "eslint ./src",
    "check:prettier": "prettier -c ./src --plugin-search-dir=.",
    "deploy:ftp": "node -r @swc-node/register deploy.ts",
    "format:lint": "eslint ./src --fix",
    "format:prettier": "prettier -w ./src --plugin-search-dir=.",
    "build": "npm-run-all clean build:*",
    "clean": "del-cli dist",
    "check": "npm-run-all --continue-on-error --parallel check:*",
    "format": "npm-run-all --continue-on-error format:*",
    "deploy": "npm-run-all build deploy:*",
    "dev": "astro dev --host",
    "preview": "astro preview"
  },
  "devDependencies": {
    "@astrojs/vue": "^2.2.1",
    "@fontsource/nunito": "^4.5.12",
    "@nanostores/persistent": "^0.7.0",
    "@nanostores/vue": "^0.6.1",
    "@swc-node/register": "^1.6.5",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "astro": "^2.5.7",
    "autoprefixer": "^10.4.14",
    "basic-ftp": "^5.0.3",
    "cssnano": "^5.1.15",
    "del-cli": "^5.0.0",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.14.1",
    "katex": "^0.16.7",
    "lodash-es": "^4.17.21",
    "nanostores": "^0.7.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.7.2",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rehype-external-links": "^2.1.0",
    "rehype-katex": "^6.0.3",
    "remark-math": "^5.1.1",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "vite": "^3.2.7",
    "vue": "^3.2.45"
  }
}
