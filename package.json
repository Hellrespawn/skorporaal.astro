{
  "name": "skorporaal.astro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:astro": "astro build",
    "build:clean": "del-cli dist",
    "check:lint": "eslint ./src",
    "check:prettier": "prettier -c ./src --plugin-search-dir=.",
    "deploy:ftp": "node -r @swc-node/register deploy.ts",
    "format:lint": "eslint ./src --fix",
    "format:prettier": "prettier -w ./src --plugin-search-dir=.",
    "webhint:webhint": "hint http://localhost:3000",
    "build": "npm-run-all build:*",
    "check": "npm-run-all --parallel check:*",
    "deploy": "npm-run-all build deploy:*",
    "dev": "astro dev --host",
    "format": "npm-run-all format:*",
    "preview": "astro preview",
    "webhint": "npm-run-all --parallel dev --race webhint:*"
  },
  "devDependencies": {
    "@astrojs/vue": "^0.5.0",
    "@fontsource/nunito": "^4.5.9",
    "@nanostores/vue": "^0.5.5",
    "@swc-node/register": "^1.5.1",
    "@tailwindcss/typography": "^0.5.4",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "astro": "1.0.0-rc.1",
    "autoprefixer": "^10.4.7",
    "basic-ftp": "^5.0.1",
    "cssnano": "^5.1.12",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.3.0",
    "hint": "^7.1.1",
    "katex": "^0.16.0",
    "nanostores": "^0.5.12",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.1.2",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rehype-external-links": "^2.0.0",
    "rehype-katex": "^6.0.2",
    "remark-math": "^5.1.1",
    "slugify": "^1.6.5",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "vue": "^3.2.37"
  }
}
