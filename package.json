{
  "name": "skorporaal.astro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:astro": "astro build",
    "check:lint": "eslint ./src",
    "check:prettier": "prettier -c ./src --plugin-search-dir=.",
    "deploy:ftp": "node -r @swc-node/register deploy.ts",
    "format:lint": "eslint ./src --fix",
    "format:prettier": "prettier -w ./src --plugin-search-dir=.",
    "webhint:webhint": "hint http://localhost:3000",
    "build": "npm-run-all clean build:*",
    "clean": "del-cli dist",
    "check": "npm-run-all --parallel check:*",
    "deploy": "npm-run-all build deploy:*",
    "dev": "astro dev --host",
    "format": "npm-run-all format:*",
    "preview": "astro preview",
    "webhint": "npm-run-all --parallel dev --race webhint:*"
  },
  "devDependencies": {
    "@astrojs/mdx": "^0.11.3",
    "@astrojs/vue": "^1.1.0",
    "@fontsource/nunito": "^4.5.10",
    "@nanostores/vue": "^0.6.0",
    "@swc-node/register": "^1.5.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "astro": "1.4.2",
    "autoprefixer": "^10.4.12",
    "basic-ftp": "^5.0.2",
    "cssnano": "^5.1.13",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.5.1",
    "katex": "^0.16.2",
    "nanostores": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.5",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rehype-external-links": "^2.0.1",
    "rehype-katex": "^6.0.2",
    "remark-math": "^5.1.1",
    "slugify": "^1.6.5",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "vue": "^3.2.40"
  },
  "dependencies": {
    "@swc/core": "^1.3.4",
    "vite": "^3.1.4"
  }
}
