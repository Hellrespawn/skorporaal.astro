---
import Feed from './FeedComponent.vue';
import Filter from '@c:Feed/FilterComponent.vue';
import { MarkdownInstance, MDXInstance } from 'astro';
import { Frontmatter } from '../../scripts/post';

export interface Props {
  instances: (MarkdownInstance<Frontmatter> | MDXInstance<Frontmatter>)[];
}

const { instances } = Astro.props as Props;
---

<Filter client:only="vue" />
<Feed instances={instances} client:only="vue" />
