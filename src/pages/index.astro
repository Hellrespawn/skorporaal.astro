---
import { getEntriesWithCategory } from '../collection';
import About from '../components/About/About.astro';
import Feed from '../components/Feed/Feed.astro';
import GridLayout from '../layouts/GridLayout.astro';

const entriesWithCategory = await getEntriesWithCategory();
---

<GridLayout title="Home">
  <Fragment slot="left">
    <About />
  </Fragment>
  <Fragment slot="right">
    <Feed entriesWithCategory={entriesWithCategory} />
  </Fragment>
</GridLayout>
