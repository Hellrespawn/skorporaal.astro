---
import { getEntries } from '../collection';
import About from '../components/About/About.astro';
import FeedWrapper from '../components/Feed/FeedWrapper.astro';
import GridLayout from '../layouts/GridLayout.astro';

const entries = await getEntries();
---

<GridLayout title="Home">
  <Fragment slot="left">
    <About />
  </Fragment>
  <Fragment slot="right">
    <FeedWrapper entries={entries} />
  </Fragment>
</GridLayout>
